 <div class="layout-row layout-xs-column">
      {{#each model as |registeredExercise| }}
        {{#paper-card class="flex-xs-100 flex-gt-xs-50" as |card|}}
          {{#card.title as |title|}}
            {{#title.text as |text|}}
            {{#text.headline}}{{registeredExercise.exercise.name}}{{/text.headline}}
            {{#text.subhead  }}
              <div class="layout-column layout-align-center-start">
                <div>Último: 120kg</div>
                <div>Máximo: 120kg</div>
              </div>
            {{/text.subhead}}
            {{/title.text}}

            {{#card.media size="xl"}}
              <div class="card-media"></div>
            {{/card.media}}
          {{/card.title}}
          {{#card.actions}}
            <!-- {{#paper-button onClick=(route-action "registerExercise" )}}Registar{{/paper-button}} -->
          {{/card.actions}}
        {{/paper-card}}
      {{/each}}
  </div>
